{
  "$schema": "http://json-schema.org/draft-07/schema",
  "description": "A taxon",
  "type": "object",
  "required": ["_key", "name"],
  "additionalProperties": false,
  "properties": {
    "_key": {
      "type": "string",
      "description": "A 16-byte blake2b hash of the name"
    },
    "name": {
      "type": "string",
      "description": "The taxon name.",
      "example": "Porphyromonas"
    }
  }
}
